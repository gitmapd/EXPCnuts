    "import matplotlib.pyplot as plt\n",
    "plt.style.use('seaborn-whitegrid')\n",
    "import numpy as np\n",
    "\n",
    "fives= np.linspace(-5,5)\n",
    "\n",
    "x = np.linspace(0, 20, num=50)\n",
    "\n",
    "y = 0.5*x + 7 + fives\n",
    "\n",
    "plt.plot(x, y, 'o', color='black');\n",
    "\n",
    "plt.show()"
   ]
